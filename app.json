{
  "expo": {
    "name": "Great Places",
    "description": "Keep in your pocket an archive of the greatest places you have visited.",
    "slug": "great-places",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/great-places-icon.png",
    "splash": {
      "image": "./assets/great-places-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/great-places-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
