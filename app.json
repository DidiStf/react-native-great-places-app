{
  "expo": {
    "name": "Great Places",
    "description": "Keep in your pocket an archive of the greatest places you have visited.",
    "slug": "great-places",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/great-places-icon.png",
    "splash": {
      "image": "./assets/great-places-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.didistf.great-places",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/great-places-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDRd2eIH9wtqVbrbl0nXXrxIPTPUDVCARg"
        }
      },
      "package": "com.didistf.great_places",
      "versionCode": 1,
      "permissions": [ "ACCESS_FINE_LOCATION", "CAMERA", "WRITE_EXTERNAL_STORAGE" ],
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
